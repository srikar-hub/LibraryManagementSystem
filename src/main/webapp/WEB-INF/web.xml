<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>LoginController</servlet-name>
  	<display-name>LoginController</display-name>
  	<description></description>
  	<servlet-class>com.controller.LoginController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>SignUpController</servlet-name>
  	<display-name>SignUpController</display-name>
  	<description></description>
  	<servlet-class>com.controller.SignUpController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>BorrowBookServlet</servlet-name>
  	<display-name>BorrowBookServlet</display-name>
  	<description></description>
  	<servlet-class>com.controller.BorrowBookServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ReturnBookServlet</servlet-name>
  	<display-name>ReturnBookServlet</display-name>
  	<description></description>
  	<servlet-class>com.controller.ReturnBookServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>LoginController</servlet-name>
  	<url-pattern>/LoginController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>SignUpController</servlet-name>
  	<url-pattern>/SignUpController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>BorrowBookServlet</servlet-name>
  	<url-pattern>/BorrowBookServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ReturnBookServlet</servlet-name>
  	<url-pattern>/ReturnBookServlet</url-pattern>
  </servlet-mapping>
</web-app>
